@model ProductItemViewModel
@{
    ViewData["Title"] = "Index";
}
<h4>Products By Brand - @Model.BrandName</h4>
<ul class="list-group col-sm-8 col-xs-12">
    @{
        foreach (ProductItem item in Model.ProductItems)
        {
            <li class="list-group-item">
                @item.Description
            </li>
        }
    }
</ul>